{
  "name": "@agelesscoding/agc-components-vue3",
  "version": "0.0.6",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "vue3"
  ],
  "description": "Vue3 Writing and Packaging AGC Project Component Library",
  "author": "smpower <rf.wangchn@foxmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/agelesscoding/agc-components-vue3"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build:esm && npm run build:umd",
    "build:esm": "rollup -c build/rollup.esm.config.js",
    "build:umd": "rollup -c build/rollup.umd.config.js",
    "clean": "rimraf dist",
    "test:watch": "vitest",
    "test": "vitest --run",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore --max-warnings 5",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run format && npm run lint && npm run test && npm run build",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^18.17.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.0",
    "rollup-plugin-css-only": "^4.3.0",
    "rollup-plugin-typescript2": "^0.35.0",
    "tslib": "^2.6.1",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.6"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
